{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini-api-key",
    "GOOGLE_CLIENT_ID": "@google-client-id",
    "GOOGLE_CLIENT_SECRET": "@google-client-secret",
    "JWT_SECRET": "@jwt-secret"
  },
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  }
}